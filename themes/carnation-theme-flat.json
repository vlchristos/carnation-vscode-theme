        "meta.object.member",
        "meta.embedded.expression"
      ],
      "settings": {
        "foreground": "#adbac7"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#dcbdfb"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "support.class.component"
