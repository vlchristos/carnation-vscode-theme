      "settings": {
        "foreground": "#96d0ff"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#6cb6ff"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#6cb6ff"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#f69d50"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#adbac7"
      }
    },
